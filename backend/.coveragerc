[run]
source = .
omit = 
    */venv/*
    */env/*
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    */node_modules/*
    setup.py
    manage.py
    */settings/*
    */config/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = AI Research Agent Coverage Report

[xml]
output = coverage.xml